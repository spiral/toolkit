{
  "name": "sf-toolkit",
  "version": "0.1.0",
  "description": "Compilation of SF-JS scripts for forms and inputs",
  "main": "resources/scripts/sf.toolkit.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:prod && npm run build:less",
    "build:prod": "cross-env NODE_ENV=production webpack -p",
    "build:dev": "cross-env NODE_ENV=development webpack -p",
    "build:less": "lessc ./js-source/styles/spiral/spiral.less ./resources/spiral.css",
    "lessc": "less",
    "clean": "rimraf /resources/scripts/sf.*.js",
    "prepublish": "npm run build",
    "clean-after-deploy": "rimraf node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spiral/toolkit.git"
  },
  "keywords": [
    "input",
    "spiral",
    "sf.js",
    "sf"
  ],
  "author": "Nataliya Zhukava <nataliya.zhukava@gmail.com>, Yauheni Yasinau  <iasinov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spiral/toolkit/issues"
  },
  "homepage": "https://github.com/spiral/toolkit#readme",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.18.0",
    "eslint-config-google": "^0.7.1",
    "eslint-loader": "^1.7.0",
    "imports-loader": "^0.6.5",
    "less": "^2.7.2",
    "less-loader": "^4.0.2",
    "node-sass": "^4.5.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.6",
    "webpack": "^2.3.1"
  },
  "dependencies": {
    "moment": "^2.18.1",
    "pikaday": "^1.5.1",
    "sf-core": "sfjs/sf.js.git#feature/node-package",
    "sf-locker": "sfjs/sf-module-locker.git#feature/node-package",
    "sf-form": "sfjs/sf-module-form.git#feature/node-package"
  }
}
